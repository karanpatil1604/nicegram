{
    "version": 2,
    "builds": [
      {
        "src": "nicegram_main/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtiem": "python3.11"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "distDir": "staticfiles_build"
      }
    ],
    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "nicegram_main/wsgi.py"
        }
      ]
  }